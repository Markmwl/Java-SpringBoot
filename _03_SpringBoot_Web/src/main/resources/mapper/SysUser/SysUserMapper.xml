<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mark.Mybatis.SysUser.SysUserMapper">

    <select id="UserLogin" resultType="com.mark.Models.SysUser">
        select t.* from T_SYSUSER t where t.name = #{name} and t.password =#{password}
    </select>
</mapper>